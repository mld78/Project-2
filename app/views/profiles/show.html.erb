<h1>Profiles#show</h1>
<p>Find me in app/views/profiles/show.html.erb</p>

<% if @user == current_user %>
  <%= link_to 'Edit Profile', edit_profile_path(@user.user_name) %>
<% end %>

<div class="image center-block">
    <%= image_tag @user.prof.url(:medium) %>
</div>
<p class="bio">
    <%= @user.bio %>
</p>

<% @couplets.each do |couplet| %>
<div class="time-ago">
  <%= time_ago_in_words couplet.created_at %>
</div>
<%= link_to (image_tag couplet.image.url(:medium), class:'img-responsive'), couplet %>
"<%= couplet.title%>"<br>
<%= couplet.first_line%><br>
<%= couplet.last_line%> <br>
By <%= couplet.user.user_name%>
<% end %>
