<%= form_for @user, url: {action: "update"}, html: {class: 'form-horizontal', multipart: true } do |f| %>

<div class='form-wrapper'>
<div class="field form-group">
    <%= f.label :prof, "Profile Picture" %><br>
    <%= f.file_field :prof %><br>
    <!-- add a text field for name -->
  </div>
  <div class="field form-group">
    <%= f.label :bio %><br>
    <%= f.text_area :bio, :placeholder => 'You need to add a bio!' %><br>
  </div>
  <div class="actions">
    <%= f.submit "Update your profile!" %>
  </div>

  <%end%>
</div>
